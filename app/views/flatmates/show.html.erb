<p id="notice"><%= notice %></p>

<p>
  <strong>Firstname:</strong>
  <%= @flatmate.firstname %>
</p>

<p>
  <strong>Lastname:</strong>
  <%= @flatmate.lastname %>
</p>

<p>
  <strong>Photo:</strong></br>
  <%= image_tag @flatmate.image.url(:medium) %>
</p>

<p>
  <strong>Common leaving out time:</strong>
  <%= @flatmate.leaving_out.strftime("%H:%M") %>
</p>

<p>
  <strong>Common arriving time:</strong>
  <%= @flatmate.ariiving.strftime("%H:%M") %>
</p>

<p>
  <strong>Duties:</strong></br>
  <%= @flatmate.duties %>
</p>

<p>
  <strong>Sum of charges:</strong>
  <%= @flatmate.charges %>
</p>

